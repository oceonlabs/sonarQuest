# Copy this file to .env.local and fill in your actual values

# Your Oracle Cloud SonarQube instance URL
VITE_SONARQUBE_URL=https://your-oracle-sonarqube-instance.com

# SonarQube user token (generate in SonarQube: Account > Security > Tokens)
VITE_SONARQUBE_TOKEN=squ_your_actual_token_here

# Enable real SonarQube data (set to 'false' for mock data)
VITE_USE_REAL_SONARQUBE=true

# Optional: Organization key (if using SonarCloud or multi-org setup)
# VITE_SONARQUBE_ORGANIZATION=your_organization_key